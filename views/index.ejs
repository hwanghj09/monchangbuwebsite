        .content-header {
            font-size: 16px;
            font-weight: bold;
            margin-bottom: 15px;
        }
        
        .post {
            border: 1px solid #eee;
            border-radius: 8px;
            margin-bottom: 20px;
            padding: 15px;
        }
        
        .post-title {
            font-size: 14px;
            margin-bottom: 10px;
        }
        
        .post-content {
